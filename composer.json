{
    "name": "jonphipps/idehelper",
    "description": "Generate a file to help IDE's like PHPStorm, NetBeans, or Sublime Text with code completion for Laravel's static facades",
    "keywords": ["laravel","laravel4","illuminate","generator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Phipps",
            "email": "jphipps@madcreek.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.*",
        "symfony/console": "2.*",
        "phpdocumentor/reflection": "dev-develop"
    },
    "autoload": {
        "classmap": [
            "src/commands"
        ],
        "psr-0": {
            "JonPhipps\\IdeHelper": "src/"
        }
    },
    "minimum-stability": "dev"
}
